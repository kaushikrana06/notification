<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/colorTextReverse" />
            <stroke android:width="1dp" android:color="@color/colorNavBack" />
            <corners android:radius="16dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/colorNavBack" />
            <stroke android:width="1dp" android:color="@color/colorNavBack" />
            <corners android:radius="16dp" />
        </shape>
    </item>
</selector>